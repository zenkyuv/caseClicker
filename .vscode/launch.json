	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Chrome 19006",
			"request": "launch",
			"type": "chrome",
			"url": "http://localhost:19006",
			"webRoot": "${workspaceFolder}"
		},

		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/s/server/websocketServer/utils/exposeViaWebsockets.ts",
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			],
			"env": { "NODE_ENV": "development" },
		}
	]
}